<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Load.
    -->
    <changeSet id="20191022021124-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="load_id"
                                 baseTableName="load_calculations"
                                 constraintName="fk_load_calculations_load_id"
                                 referencedColumnNames="id"
                                 referencedTableName="load"/>

        <addForeignKeyConstraint baseColumnNames="calculations_id"
                                 baseTableName="load_calculations"
                                 constraintName="fk_load_calculations_calculations_id"
                                 referencedColumnNames="id"
                                 referencedTableName="calculation"/>
        
    </changeSet>
</databaseChangeLog>
