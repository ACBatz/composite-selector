<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity EnvironmentalEffect.
    -->
    <changeSet id="20191022021125-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="environmental_effect_id"
                                 baseTableName="environmental_effect_calculations"
                                 constraintName="fk_environmental_effect_calculations_environmental_effect_id"
                                 referencedColumnNames="id"
                                 referencedTableName="environmental_effect"/>

        <addForeignKeyConstraint baseColumnNames="calculations_id"
                                 baseTableName="environmental_effect_calculations"
                                 constraintName="fk_environmental_effect_calculations_calculations_id"
                                 referencedColumnNames="id"
                                 referencedTableName="calculation"/>
        
    </changeSet>
</databaseChangeLog>
