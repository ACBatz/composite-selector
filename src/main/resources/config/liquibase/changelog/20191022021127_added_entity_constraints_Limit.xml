<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Limit.
    -->
    <changeSet id="20191022021127-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="limit_id"
                                 baseTableName="jhi_limit_calculations"
                                 constraintName="fk_jhi_limit_calculations_jhi_limit_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_limit"/>

        <addForeignKeyConstraint baseColumnNames="calculations_id"
                                 baseTableName="jhi_limit_calculations"
                                 constraintName="fk_jhi_limit_calculations_calculations_id"
                                 referencedColumnNames="id"
                                 referencedTableName="calculation"/>
        
    </changeSet>
</databaseChangeLog>
